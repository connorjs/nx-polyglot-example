{
	"$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
	"name": "root",
	"targets": {
		"ci": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"nx run format",
					"nx run-many --targets build,lint,test --exclude root"
				]
			}
		},
		"format": {
			"cache": true,
			"command": "echo 'format'"
		},
		"lint": {
			"command": "nx run-many -t lint --exclude root"
		}
	}
}
