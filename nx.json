{
	"$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/nx-schema.json",
	"affected": {
		"defaultBase": "main"
	},
	"targetDefaults": {
		"build": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"ci": {
			"cache": false,
			"dependsOn": [ "build", "format", "lint", "test" ]
		},
		"codegen": {
			"cache": true,
			"dependsOn": [ "^codegen" ]
		},
		"dev": {
			"cache": false,
			"dependsOn": [ "codegen" ]
		},
		"format": {
			"cache": true
		},
		"lint": {
			"cache": true,
			"dependsOn": [ "lint:cs", "lint:es", "lint:ss", "lint:tc" ]
		},
		"lint:cs": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"lint:es": {
			"cache": true,
			"dependsOn": [ "codegen" ],
			"executor": "nx:run-commands",
			"options": {
				"command": "eslint -f pretty ."
			},
			"outputs": [ "{options.outputFile}" ]
		},
		"lint:es:fix": {
			"cache": true,
			"dependsOn": [ "codegen" ],
			"executor": "nx:run-commands",
			"options": {
				"command": "eslint -f pretty . --fix"
			},
			"outputs": [ "{options.outputFile}" ]
		},
		"lint:fix": {
			"cache": true,
			"dependsOn": [ "lint:es:fix", "lint:ss:fix" ]
		},
		"lint:ss": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"lint:ss:fix": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"lint:tc": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"test": {
			"cache": false,
			"dependsOn": [ "test:unit", "test:integ", "test:system" ]
		},
		"test:unit": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"test:integ": {
			"cache": true,
			"dependsOn": [ "codegen" ]
		},
		"test:system": {
			"cache": false,
			"dependsOn": [ "codegen", "build" ]
		}
	}
}
